!!! 5
	html
		head
			import components/bootstrap
		body.bg-dark
			import components/navbar
				+setnav("EloRating")
				+setnav(KitName)
			div.container
				a.btn.btn-default[href="/elo/addmatch/" + KitName] Record Match
				each $result in Players
					div.player
						div Player : #{$result.GameName}
						div Number of Matches : #{$result.Matches}
						div Rating : #{$result.Rating}
						hr
